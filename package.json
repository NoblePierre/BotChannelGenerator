{
	"name": "bot-channel-generator",
	"version": "1.0",
	"description": "",
	"main": "./src/main.ts",
	"scripts": {
		"build": "tsc --build",
		"build:clean": "tsc --build --clean",
		"docker:build": "bash ./build-docker-image.sh",
		"docker:push": "bash ./push-docker-image.sh",
		"docker:compose:up": "docker compose --env-file .env.local up -d",
		"start": "npm run env:local ts-node src/main.ts",
		"start:prod": "node dist/main.js",
		"dev": "npm run env:local ts-node-dev --respawn src/main.ts",
		"env:local": "env-cmd -f .env.local"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^18.0.6",
		"discord.js": "^14.12.1",
		"node-fetch": "^3.2.6"
	},
	"devDependencies": {
		"env-cmd": "^10.1.0",
		"eslint": "^8.46.0",
		"ts-node-dev": "^2.0.0"
	}
}
